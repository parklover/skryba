</div>
<div class="form-group form-material floating" data-plugin="formMaterial">
    {{ form_errors(form.nazwaFirmy) }}
    {{ form_widget(form.nazwaFirmy, {'attr':{'class':'form-control'}}) }}
    {{ form_label(form.nazwaFirmy, null,{'label_attr':{'class':'floating-label'} }) }}
</div>
<div class="form-group form-material floating" data-plugin="formMaterial">
    {{ form_errors(form.nazwaSerwisu) }}
    {{ form_widget(form.nazwaSerwisu, {'attr':{'class':'form-control'}}) }}
    {{ form_label(form.nazwaSerwisu, null,{'label_attr':{'class':'floating-label'} }) }}
</div>
<div class="form-group form-material floating" data-plugin="formMaterial">
